{
    "name": "amphetkid/laravel-zoom",
    "description": "Laravel Zoom Api",
    "license": "MIT",
    "authors": [
        {
            "name": "Max Drax",
            "email": "drax@numnuc.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "guzzlehttp/guzzle": "~7.0|~6.0|~5.0|~4.0",
        "firebase/php-jwt": "^5.0",
        "illuminate/support": "^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Amphetkid\\Zoom\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Amphetkid\\Zoom\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Amphetkid\\Zoom\\Providers\\ZoomServiceProvider"
            ]
        }
    },
    "minimum-stability" : "dev"
}
